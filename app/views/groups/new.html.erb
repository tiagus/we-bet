<h1>Create Group</h1>

<%= simple_form_for @group do |f| %>
  <%= f.input :name, label: 'Name of Group' %>
  <%= f.input :description %>
  <%= f.input :public %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Create", class: "btn btn-primary" %>
<% end %>
<%= link_to 'Back', groups_path, class: 'btn btn-primary' %>
